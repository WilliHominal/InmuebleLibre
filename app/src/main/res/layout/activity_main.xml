<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <!--Boton de prueba (eliminar despues)-->
    <Button
        android:id="@+id/prueba_agregar_inmueble_BTN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Probar Agregar Inmueble"
        app:layout_constraintVertical_chainStyle="spread"
        app:layout_constraintBottom_toTopOf="@id/prueba_registrarse_BTN"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!--Boton de prueba (eliminar despues)-->
    <Button
        android:id="@+id/prueba_registrarse_BTN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Probar Registro"
        app:layout_constraintTop_toBottomOf="@id/prueba_agregar_inmueble_BTN"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>